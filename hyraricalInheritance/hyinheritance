class Parent(
    public void add(){
        System.out.println("HELLO WORLD");
    }
)
class child extends Parent{
    void display(){
        add();
    }
}
class  extends parent{
    void helper(){
        display()
    }
}
public class  hyinheritance{
    public static void main(String arg[]){
    Child obj = new child();
    obj.helper();
    child obj = new child();
    obj.display();
}
}